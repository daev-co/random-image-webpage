{
    "family": "daevco-random-image-webpage-ecs-task-definition",
    "containerDefinitions": [
        {
            "name": "daevco-random-image-webpage-ecs-fargate-container",
            "image": "125765595335.dkr.ecr.us-east-1.amazonaws.com/random-image-webpage:b5b3751f2d9df8fdaf5174947d41bf2153605d20",
            "cpu": 256,
            "memory": 300,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "startTimeout": 30,
            "stopTimeout": 30,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/daevco-random-image-webpage-ecs-logging-group",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::125765595335:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::125765595335:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "healthCheck": {
        "command": [
            "CMD-SHELL",
            "curl -f http://localhost/ || exit 1"
        ],
        "interval": 10,
        "timeout": 5,
        "retries": 5,
        "startPeriod": 15
    }
}