{
    "family": "daevco-random-image-webpage-task-definition",
    "containerDefinitions": [
        {
            "name": "daevco-random-image-webpage-ecs-container",
            "image": "125765595335.dkr.ecr.us-east-1.amazonaws.com/random-image-webpage:ed93fb41ba94d9a61920b3df684e2e8d5a7cc2bd",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "daevco-random-image-webpage-ecs-container-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/daevco-random-image-webpage-ecs-logging-group",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost/ || exit 1"
                ],
                "interval": 5,
                "timeout": 10,
                "retries": 5,
                "startPeriod": 10
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::125765595335:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::125765595335:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}